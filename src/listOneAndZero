import java.util.Scanner;
import java.util.ArrayList;
/**
*Write a program that reads a natural number n from the keyboard and creates a list in which the first and last elements will be equal to the string 1, 
*and between them there will be n 0. Display the list on the screen separated by a space.
*/
class listOneAndZero{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a = 0;

      ArrayList<String> list = new ArrayList<>();
        list.add(0,"1");

        while (a < n)
        {
            list.add("0");
            a++;
        }
        list.add("1");
        String formattedString = list.toString()
                .replace(",", "")  //remove the commas
                .replace("[", "")  //remove the right bracket
                .replace("]", "")  //remove the left bracket
                .trim();
        System.out.println(formattedString);
    }
}
